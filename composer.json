{
    "require": {
        "martin-georgiev/postgresql-for-doctrine": "^2.10",
        "ext-pdo": "*",
        "vlucas/phpdotenv": "^5.6",
        "guzzlehttp/psr7": "^2.7",
        "twig/twig": "^3.20"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.73",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "cs-fix": "./vendor/bin/php-cs-fixer fix --diff --allow-risky=yes",
        "phpstan": "php -d memory_limit=-1 ./vendor/bin/phpstan analyse"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
